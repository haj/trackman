// Append new data
$("#companies-tbody").append("<%= j render(:partial => '/companies/partials/companies_rows', locals: { companies: @companies }) %>")

// Update pagination link
<% if @companies.last_page? %>
  $('.pagination').html("No More Data to Show!");
<% else %>
  $('.pagination')
    .html("<%=j link_to_next_page(@companies, 'Load more data', :remote => true) %>");
<% end %>
