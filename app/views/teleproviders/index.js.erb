// Append new data
$("#teleproviders-tbody").append("<%= j render(:partial => '/teleproviders/partials/teleproviders_rows', locals: { teleproviders: @teleproviders }) %>")

// Update pagination link
<% if @teleproviders.last_page? %>
  $('.pagination').html("No More Data to Show!");
<% else %>
  $('.pagination')
    .html("<%=j link_to_next_page(@teleproviders, 'Load more data', :remote => true) %>");
<% end %>
