// Append new data
$("#alarms-tbody").append("<%= j render(:partial => '/alarms/partials/alarms_rows', locals: { alarms: @alarms }) %>")

// Update pagination link
<% if @alarms.last_page? %>
  $('.pagination').html("No More Data to Show!");
<% else %>
  $('.pagination')
    .html("<%=j link_to_next_page(@alarms, 'Load more data', :remote => true) %>");
<% end %>
