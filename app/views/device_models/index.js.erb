// Append new data
$("#device-models-tbody").append("<%= j render(:partial => '/device_models/partials/device_models_rows', locals: { device_models: @device_models }) %>")

// Update pagination link
<% if @device_models.last_page? %>
  $('.pagination').html("No More Data to Show!");
<% else %>
  $('.pagination')
    .html("<%=j link_to_next_page(@device_models, 'Load more data', :remote => true) %>");
<% end %>
