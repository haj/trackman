= form_for @work_schedule_group do |f|
  - if @work_schedule_group.errors.any?
    #error_explanation
      %h2= "#{pluralize(@work_schedule_group.errors.count, "error")} prohibited this work_schedule_group from being saved:"
      %ul
        - @work_schedule_group.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :company_id
    = f.number_field :company_id
  .field
    = f.label :work_schedule_id
    = f.number_field :work_schedule_id
  .actions
    = f.submit 'Save'
