// Append new data
$("#car-types-tbody").append("<%= j render(:partial => '/car_types/partials/car_types_rows', locals: { car_types: @car_types }) %>")

// Update pagination link
<% if @car_types.last_page? %>
  $('.pagination').html("No More Data to Show!");
<% else %>
  $('.pagination')
    .html("<%=j link_to_next_page(@car_types, 'Load more data', :remote => true) %>");
<% end %>
