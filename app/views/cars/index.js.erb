// Append new data
$("#cars-tbody").append("<%= j render(:partial => '/cars/partials/cars_row', locals: { cars: @cars }) %>")

// Update pagination link
<% if @cars.last_page? %>
  $('.pagination').html("No More Data to Show!");
<% else %>
  $('.pagination')
    .html("<%=j link_to_next_page(@cars, 'Load more data', :remote => true) %>");
<% end %>
