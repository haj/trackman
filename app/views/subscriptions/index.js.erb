// Append new data
$("#subscriptions-tbody").append("<%= j render(:partial => '/subscriptions/partials/subscriptions_rows', locals: { subscriptions: @subscriptions }) %>")

// Update pagination link
<% if @subscriptions.last_page? %>
  $('.pagination').html("No More Data to Show!");
<% else %>
  $('.pagination')
    .html("<%=j link_to_next_page(@subscriptions, 'Load more data', :remote => true) %>");
<% end %>
