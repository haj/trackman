// Append new data
$("#car-manufacturers-tbody").append("<%= j render(:partial => '/car_manufacturers/partials/car_manufacturers_rows', locals: { car_manufacturers: @car_manufacturers }) %>")

// Update pagination link
<% if @car_manufacturers.last_page? %>
  $('.pagination').html("No More Data to Show!");
<% else %>
  $('.pagination')
    .html("<%=j link_to_next_page(@car_manufacturers, 'Load more data', :remote => true) %>");
<% end %>
