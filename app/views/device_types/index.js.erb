// Append new data
$("#device-types-tbody").append("<%= j render(:partial => '/device_types/partials/device_types_rows', locals: { device_types: @device_types }) %>")

// Update pagination link
<% if @device_types.last_page? %>
  $('.pagination').html("No More Data to Show!");
<% else %>
  $('.pagination')
    .html("<%=j link_to_next_page(@device_types, 'Load more data', :remote => true) %>");
<% end %>
