// Append new data
$("#devices-tbody").append("<%= j render(:partial => '/devices/partials/devices_rows', locals: { devices: @devices }) %>")

// Update pagination link
<% if @devices.last_page? %>
  $('.pagination').html("No More Data to Show!");
<% else %>
  $('.pagination')
    .html("<%=j link_to_next_page(@devices, 'Load more data', :remote => true) %>");
<% end %>
